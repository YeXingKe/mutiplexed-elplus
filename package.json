{
  "name": "mutiplexed-elplus",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "build:components": "node ./command/build.js",
    "lib": "yarn build:components && cp package.json",
    "prepare": "husky install",
    "lint": "eslint src/**/*.{js,jsx,vue,ts,tsx} --fix && eslint packages/**/*.{js,jsx,vue,ts,tsx} --fix",
    "prettier": "prettier --write src/**/*.{js,jsx,vue,ts,tsx} && prettier --write packages/**/*.{js,jsx,vue,ts,tsx}"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@fullcalendar/common": "^5.11.4",
    "@fullcalendar/core": "^6.1.4",
    "@fullcalendar/daygrid": "^6.1.4",
    "@fullcalendar/interaction": "^6.1.4",
    "@fullcalendar/list": "^6.1.4",
    "@fullcalendar/timegrid": "^6.1.4",
    "@fullcalendar/vue3": "^6.1.4",
    "@types/file-saver": "^2.0.5",
    "@types/lodash": "^4.14.191",
    "axios": "^1.3.4",
    "babel-eslint": "^10.1.0",
    "element-plus": "^2.2.26",
    "file-saver": "^2.0.5",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "path": "^0.12.7",
    "plupload": "^2.3.9",
    "spark-md5": "^3.0.2",
    "tippy.js": "^6.3.7",
    "vue": "^3.2.41",
    "vue-router": "^4.0.13",
    "wangeditor": "^4.7.15",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/node": "^18.14.5",
    "@types/plupload": "^2.0.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.57.0",
    "sass-loader": "^13.2.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-checker": "^0.5.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-externals": "^0.6.2",
    "vite-plugin-windicss": "^1.8.10",
    "vue-tsc": "^1.0.9"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "packages/**/*.{js,jsx,ts,tsx,json,css,scss,md,vue}": [
      "yarn lint"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,vue}": [
      "yarn lint"
    ]
  }
}
